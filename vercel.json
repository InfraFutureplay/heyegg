{
    "version": 2,
    "buildCommand": "npm run start",
    "framework": "other",
    "builds": [
        {
            "src": "src/server.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/src/api/$1",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE",
                "OPTIONS"
            ]
        },
        {
            "src": "/(.*)",
            "dest": "src/server.ts"
        }
    ]
}